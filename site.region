<!doctype html>
<html>
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="initial-scale=1">
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@100;200;300;400;500;600;700;800;900&display=swap" rel="stylesheet">

    <!--Include sqs scripts, meta tags, and user content from the code injection tab.-->
    {squarespace-headers}
  </head>

  <body>
    <div id="header">
        <div class="container">
            <h2 data-content-field="site-title">{website.siteTitle}</h2>

            <div id="nav-icon">
                <div class="nav-icon--top"></div>
                <div class="nav-icon--bottom"></div>
            </div>
        </div>
    </div>

    <div id="navigation-menu">
        <squarespace:navigation navigationId="navigation" template="navigation" />
    </div>

    <main role="main" data-content-field="main-content">{squarespace.main-content}</main>


    <div class="footer">
        <squarespace:navigation navigationId="socialnav" template="socialnav" />
        <squarespace:navigation navigationId="navigation" template="navigation" />

        {#  <div class="search">
            <div class="search__cursor"></div>
            <squarespace:block-field id="search--sitewide" columns="1" />
        </div>  }
    </div>

    <!--Injection point for tracking scripts and user content from the code injection tab.-->
    {squarespace-footers}

  </body>
</html>

<squarespace:script src="style.js" combo="false" />
    <script>
        navIcon.addEventListener("click", () => {
            toggleNavigation()
        })
    </script>
